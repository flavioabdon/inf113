
CREATE TABLE message (
	id INT(6) PRIMARY_KEY AUTO_INCREMENT,
	contac VARCHAR(50) NOT NULL,
	msg VARCHAR(300) NOT NULL,
	status	INT(6) NOT NULL DEFAULT 0
);



con este si funciono 

CREATE TABLE messages ( contac VARCHAR(50), msg VARCHAR(300), status CHAR(1) );


