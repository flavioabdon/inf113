<% include partials/_header %>

<div class="container mt-5">
  <div class="row">
    <div class="col-md-5 offset-md-3 card">
      <div class="card-body">
        <form action="/contrato/updateCon/<%= data.id %>" method="POST">
          <div class="form-group">
            <input type="text" name="id_inq" value="<%= data.id_inq%>" placeholder="Name" class="form-control" autofocus>
          </div>
          <div class="form-group">
            <input type="text" name="id_amb" value="<%= data.id_amb%>" placeholder="Address" class="form-control">
          </div>
          <div class="form-group">
            <input type="text" name="fecha_ini" value="<%= data.fecha_ini%>" placeholder="Phone" class="form-control">
          </div>
          <div class="form-group">
            <input type="text" name="fecha_fima" value="<%= data.fecha_fima%>" placeholder="Phone" class="form-control">
          </div>

          <div class="form-group">
            <input type="text" name="data.garantia" value="<%= data.garantia%>" placeholder="Phone" class="form-control">
          </div>

          <button type="submit" class="btn btn-primary">
            save customer
          </button>
        </form>
      </div>
    </div>
  </div>
</div>

<% include partials/_footer %>
