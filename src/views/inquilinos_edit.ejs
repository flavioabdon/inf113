<% include partials/_header %>

<div class="container mt-5">
  <div class="row">
    <div class="col-md-5 offset-md-3 card">
      <div class="card-body">
        <form action="/inquilino/updateInq/<%= data.id %>" method="POST">
          <div class="form-group">
            <input type="text" name="ci" value="<%= data.ci%>" placeholder="Name" class="form-control" autofocus>
          </div>
          <div class="form-group">
            <input type="text" name="extencion" value="<%= data.extencion%>" placeholder="Address" class="form-control">
          </div>
          <div class="form-group">
            <input type="text" name="nombre" value="<%= data.nombre%>" placeholder="Phone" class="form-control">
          </div>
          <div class="form-group">
            <input type="text" name="cel" value="<%= data.cel%>" placeholder="Phone" class="form-control">
          </div>
          <div class="form-group">
            <input type="text" name="genero" value="<%= data.genero%>" placeholder="Phone" class="form-control">
          </div>
          <div class="form-group">
            <input type="text" name="fecha" value="<%= data.fecha%>" placeholder="Phone" class="form-control">
          </div>

          <button type="submit" class="btn btn-primary">
            save customer
          </button>
        </form>
      </div>
    </div>
  </div>
</div>

<% include partials/_footer %>
