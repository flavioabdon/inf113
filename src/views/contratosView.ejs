<% include partials/_header %>

<div class="container mt-5">
  <div class="row">
    <div class="col-md-10">
      <table class="table table-bordered table-hover">
        <thead>
          <tr class="table-tr">
            <td>Nro</td>
            <td>Inquilino</td>
            <td>Ambiente</td>
            <td>F. Inicial</td>
            <td>F Conclusion</td>
            <td>F. Firma</td>
            <td>Garantia</td>
            <td>Acciones</td>
          </tr>
        </thead>
        <tbody>
          <% if (data) { %>
            <% for(var i = 0; i < data.length; i++) { %>
              <tr>
                <td><%= (i + 1) %></td>
                <td><%= data[i].id_inq %></td>
                <td><%= data[i].id_amb %></td>
                <td><%= data[i].fecha_ini %></td>
                <td><%= data[i].fecha_fin %></td>
                <td><%= data[i].fecha_fima %></td>
                <td><%= data[i].garantia %></td>
                <td>
                    <a href="contrato/updateCon/<%= data[i].id %>" class="btn btn-sm btn-outline-info">
                      <img src="data:image/svg+xml;utf8;base64," />
                      </a>
                    <a href="contrato/deleteCon/<%= data[i].id %>" class="btn btn-sm btn-outline-danger">
                      <img src="data:image/svg+xml;utf8;base64," />
                    </a>
                </td>
              </tr>
            <% } %>
          <% } %>
        </tbody>
      </table>
    </div>
    <div class="col-md-2">
      <button type="button" class="btn btn-outline-info" data-toggle="modal" data-target="#exampleModal">
        Adicionar Contrato
      </button>
    </div>
  </div>
</div>
<div class="">
  <% include partials/_footer %>
</div>
<!-- Modal -->
<div class="modal fade" id="exampleModal" tabindex="-1" role="dialog" aria-labelledby="exampleModalLabel" aria-hidden="true">
  <div class="modal-dialog" role="document">
    <div class="modal-content">
      <div class="modal-header">
        <h5 class="modal-title" id="exampleModalLabel">Agregar Ambiente</h5>
        <button type="button" class="close" data-dismiss="modal" aria-label="Close">
          <span aria-hidden="true">&times;</span>
        </button>
      </div>
      <div class="modal-body">
        <div class="card-body">
          <form action="/contrato/addCon" method="POST">
            <div class="form-group">
              <input type="text" name="id_inq" placeholder="Inquilino" class="form-control" autofocus>
            </div>
            <div class="form-group">
              <input type="text" name="id_amb" placeholder="Ambiente" class="form-control">
            </div>
            <div class="form-group">
              <input type="text" name="fecha_ini" placeholder="F. Inicio" class="form-control">
            </div>
            <div class="form-group">
              <input type="text" name="fecha_fin" placeholder="F. Conclusion" class="form-control">
            </div>
            <div class="form-group">
              <input type="text" name="fecha_fima" placeholder="F. Firma" class="form-control">
            </div>
            <div class="form-group">
              <input type="text" name="garantia" placeholder="Garantia" class="form-control">
            </div>
            <button type="submit" class="btn btn-primary">
                <img src="data:image/svg+xml;utf8;base64," />
                Guardar contrato
            </button>
          </form>
        </div>
      </div>
    </div>
  </div>
</div>
